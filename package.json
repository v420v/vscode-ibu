{
  "name": "ibu-syntax-highlighter",
  "displayName": "Ibu Programming Language Syntax Highlighter",
  "description": "Syntax highlighting for the Ibu programming language",
  "version": "0.0.2",
  "publisher": "ibu",
  "keywords": [
    "syntax highlighter",
    "The Ibu programming language",
    "Ibu",
    "programming language"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/v420v/vscode-ibu"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ibu",
        "aliases": [
          "Ibu",
          "ibu"
        ],
        "extensions": [
          ".ibu"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ibu",
        "scopeName": "source.ibu",
        "path": "./syntaxes/ibu.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
